@using BioDesigner.Client.Models;

<div class="circuit-viewer">
    @foreach (var device in GeneticCircuit.Devices)
    {
        <GeneticDeviceCard GeneticDevice="@device" />
    }
</div>

@code {
    [Parameter] public GeneticCircuit GeneticCircuit { get; set; } = new GeneticCircuit(Guid.NewGuid(), "New Circuit", new List<GeneticDevice>());
}
